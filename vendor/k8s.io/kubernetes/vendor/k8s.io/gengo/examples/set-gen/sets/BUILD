    name = "go_default_library",
    srcs = [
        "byte.go",
        "doc.go",
        "empty.go",
        "int.go",
        "int64.go",
        "string.go",
    ],
    importpath = "k8s.io/gengo/examples/set-gen/sets",
    visibility = ["//visibility:public"],
)

